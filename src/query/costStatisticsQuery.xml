<?xml version="1.0" encoding="UTF-8"?>
<pmsquery>


    <query id="costStatisticsQuery" header="成本录入统计" method="costStatisticsManager.selectCostStatistics">
        <column key="id" header="主键" type="java.lang.Long" operator="in" align="center" allowSort="false" display="false"/>
        <column key="cityName" header="城市名称" type="java.lang.String" operator="eq" align="center" allowSearch="false" allowSort="false" display="true"  width="4%"/>
        <column key="cityId" header="城市ID" type="java.lang.String" operator="eq" align="center" allowSearch="false" allowSort="false" display="false" />
        <column key="storeNo" header="门店编码" type="java.lang.String" operator="eq" align="center" allowSearch="false" allowSort="false" display="false" />
        <column key="year" header="年" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="false" />
        <column key="month" header="月" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="false"/>
        <column key="total" header="总门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="3%"/>
        <column key="labor" header="人工成本-员工薪酬录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="8%"/>
        <column key="gwe" header="水电费录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="5%"/>
        <column key="rent" header="租金成本-物业费用录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="7%"/>
        <column key="operation" header="运营成本录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="5%"/>
        <column key="uniform" header="人工成本-工服费用录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="7%"/>
        <column key="renovation" header="装修摊销录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="5%"/>
        <column key="fixedAsset" header="固定资产录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="4%"/>
        <column key="rentContract" header="租金成本-合同租金录入门店数" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="false" display="true" width="6%"/>
    </query>

    <query id="queryCostCity" header="城市"  order="id desc" method="noticeManager.getNoticeCity">
        <column key="id" header="选择" display="true" type="java.lang.Long" allowSearch="true" allowSort="false" align="center" width="10%"/>
        <column key="cityname" header="城市名称"  operator="like" align="center" allowSort="true" type="String" width="45%"/>
        <column key="citycode" header="城市编码"  align="center" allowSort="true" type="String" width="45%"/>

    </query>
</pmsquery>