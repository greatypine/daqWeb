<?xml version="1.0" encoding="UTF-8"?>
<pmsquery>


    <query id="costControlQuery" header="成本控制列表" baseclass="com.cnpc.pms.costStatistics.entity.CostControl"  order="id desc"  filter="status = 0">
        <column key="id" header="主键" type="java.lang.Long" operator="in" align="center" allowSort="true" display="false"/>
        <column key="cityName" header="城市名称" type="java.lang.String" operator="eq" align="center" allowSearch="false" allowSort="true" display="true"  width="10"/>
        <column key="year" header="年" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="8%"/>
        <column key="month" header="月" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="5%"/>
        <column key="labor" header="人工成本" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="rent" header="租金成本" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="renovation" header="装修摊销" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="fixedAsset" header="固定资产" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="gwe" header="水电费" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="operation" header="运营" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="true" width="10%"/>
        <column key="code" header="编号" type="java.lang.String" align="center" allowSearch="false" operator="like" allowSort="true" display="false" width="10%"/>
    </query>


</pmsquery>